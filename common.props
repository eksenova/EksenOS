<Project>
	<PropertyGroup>
		<LangVersion>latest</LangVersion>
		<TargetFramework>net10.0</TargetFramework>
		<OutputType>Library</OutputType>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>nullable</WarningsAsErrors>
		<GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
		<IsPackable>true</IsPackable>
		<Version>0.0.0</Version>
		<GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
		<RepositoryUrl>https://github.com/eksenova/EksenOS</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>eksenova eksen library .net dotnet framework</PackageTags>
		<PackageProjectUrl>https://eksenova.com</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Copyright>Copyright © EKSENOVA TEKNOLOJİ ANONİM ŞİRKETİ</Copyright>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Description>EksenOS - Eksenova Open-Source Code Libraries</Description>
	</PropertyGroup>

	<ItemGroup>
		<None Include="$(MSBuildThisFileDirectory)\$(PackageReadmeFile)" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>